<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core">
   <Dialog
      id="clickDialog"
      title="다이얼로그 {/recipient/name}">
      <core:Icon
            src="sap-icon://accessibility"
            size="4rem"
            class="sapUiMediumMargin"/>
            <Text text="{i18n>innerDialogText}"></Text>
      <beginButton>
        <Button text="{i18n>dialogCloseButtonText}" press=".onCloseDialog"/>
      </beginButton>
    </Dialog>
</core:FragmentDefinition>